#!/home/cai-navi/Term_DL/term/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'lit==16.0.3','console_scripts','lit'
__requires__ = 'lit==16.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lit==16.0.3', 'console_scripts', 'lit')()
    )
